# MyB auto_ip.sh
* * * * * env PATH=/bin:/sbin:/usr/bin:/usr/sbin:/usr/local/bin:/usr/local/sbin timeout 59 nice -n 19 /usr/bin/lockf -s -t0 /tmp/auto_ip.sh.lock /root/bin/auto_ip.sh > /dev/null 2>&1
